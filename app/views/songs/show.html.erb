<%= render "navigate" %> <br> 

<h1>SONG SHOW PAGE</h1>
<p>My Song</p>
  <%= @song.name %>
  <%= link_to @song.artist, artist_path(@song.artist) %>
  <%= link_to @song.genre, artist_path(@song.genre) %>
<br>
<%= link_to "Edit #{@song.name}", edit_song_path(@song) %>
<br>
<%= button_to "Delete #{@song.name}", songs_path(@song), method: :delete %>
<br>